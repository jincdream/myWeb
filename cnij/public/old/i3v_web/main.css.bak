@charset "utf-8";
/* CSS Document */
@font-face{
	font-family:fBack;
	src:url(black.TTF);
	}
body,ul,h1,h2,h3,h4,h5,h6,p,input,img,a{
	text-decoration:none;
	border:0;
	padding:0;
	margin:0;
	list-style:none;
	}
body{
	background:rgba(194,193,193,.4);
	font-family:"微软雅黑";
	}
::-moz-selection{
background:#679B15;
color:#fff;
}
::selection{
background:#679B15;
color:#fff;
}
/*header begin================================*/
#header{
	height:50px;
	background:-moz-linear-gradient(90deg,rgba(236,108,10,1),rgba(236,108,0,.8));
	background:-webkit-gradient(linear,left bottom,left top,from(rgba(236,108,10,1)),to(rgba(236,108,0,.8)));
	border-bottom:rgba(236,108,40,1) 2px solid;
	z-index:2;
	position:relative;
	}
#header a{
	color:#fff;
	}
.nav{
	width:860px;
	margin:0 auto;	
	padding-left:100px;
	height:50px;
	background:url(logo.png) no-repeat;
	}
.mainUl{
	float:right;
	}
.mainUl li{
	float:left;
	font-size:16px;
	font-family:fBack;
	color:#fff;
	transition:all .2s ease-out;
	text-align:center;
	height:28px;
	padding:8px 0 0 0;
	}

.mainUl li:nth-of-type(1),
.mainUl li:nth-of-type(2),
.mainUl li:nth-of-type(3),
.mainUl li:nth-of-type(4),
.mainUl li:nth-of-type(5){
	width:80px;
	margin:10px 5px 0 0;
	}
.mainUl li:nth-of-type(6),
.mainUl li:nth-of-type(7){
	margin:10px 5px 0 5px;
	}
.mainUl li:nth-of-type(6){
	width:200px;
	}
.mainUl li:nth-of-type(7) input{
	background:#fff url(img/Search.png) no-repeat right;
	padding:2px;
	}
.mainUl li:nth-of-type(7) input:fouce{
	}
.mainUl li:nth-of-type(1):hover,
.mainUl li:nth-of-type(2):hover,
.mainUl li:nth-of-type(3):hover,
.mainUl li:nth-of-type(4):hover,
.mainUl li:nth-of-type(5):hover,.mainUl div{
	background:#222;
	color:#FFF !important;
	}
.mainUl div{
	padding-top:5px;
	height:0;
	overflow:hidden;
	margin-top:14px;
	transition:all .3s linear;
	border-radius:0 0 8px 8px;
	}
.mainUl div p{
	padding:5px 0;
	border-bottom:1px #999 solid;
	transition:all .3s linear;
	}
.mainUl div p:hover{
	background:#679B15;
	}
.mainUl span{
	transition:all .4s ease;
	}
.mainUl span:hover{
	color:#222;
	cursor:pointer;
	}
.nHoverA{
	height:70px !important;
	}
.nHoverB{
	height:100px !important;
	}
/*bannar================================*/
#content{
	font-family:fBack;
	width:960px;
	margin:0 auto;
	height:350px;
	padding-top:30px;
	background:#fff;
	position:relative;
	}
.leftNav{
	width:450px;
	float:left;
	}
.leftMainNav{
	width:170px;
	}
.leftMainNav li{
	font-size:16px;
	width:109px;
	}
.leftNav li:hover{
	background:#679B15 !important;
	color:#fff;
	}
.leftNav ul li{
	background:rgba(34,34,34,1);
	height:34px;
	padding:15px 25px 0 35px;
	border-bottom:rgba(102,102,102,.5) 1px solid;
	border-right:rgba(102,102,102,.5) 1px solid;
	color:#F1F1F1;
	transition:all .4s ease-in;
	}
.leftNav ul{
	position:absolute;
	float:left;
	display:none;
	left:170px;
	z-index:2;
	}
.leftNav ul:nth-of-type(1){
	left:0;
	}
.leftNav ul:nth-of-type(3){
	top:80px;
	}
.leftNav ul:nth-of-type(4){
	top:130px;
	}
.leftNav ul:nth-of-type(5),
.leftNav ul:nth-of-type(7){
	top:180px;
	}
.leftNav ul:nth-of-type(6),
.leftNav ul:nth-of-type(8){
	top:230px;
	}
.on,.leftMainNav{
	display:block !important;
	}
/*bannar================================*/
.bannar{
	position:absolute;
	left:170px;
	}
.banTitle{
	position:absolute;
	height:350px;
	width:790px;
	opacity:.2;
	transition:all ease-in .6s;
	}
.banTitle:hover{
	opacity:.8;
	}
.bannar img{
	position:absolute;
	opacity:0;
	transition:all .5s ease-in;
	}
.onImg{
	opacity:1 !important;
	}
.banTitle p,.banTitle div{
	position:absolute;
	}
.banTitle p{
	display:block;
	background:rgba(0,0,0,1);
	right:0;
	}
.banTitle p:first-of-type{/*title*/
	padding:5px 20px 5px 10px;
	bottom:80px;
	font-size:30px;
	}
.banTitle p:last-of-type{
	width:220px;
	padding:5px 120px 5px 10px;
	bottom:50px;
	}
.banTitle p a{
	color:#fff;
	}
/*tab A*/
.tab{
	width:960px;
	margin:0 auto;
	padding-top:10px;
	background:#fff;
	position:relative;
	}
.tabA{
	width:621px;
	height:400px;
	color:#666;
	float:left;
	margin-right:10px;
	}
.tabA ul{
	height:40px;
	font-family:fBack;
	}
.tabA ul li{
	float:left;
	height:25px;
	width:172px;
	font-size:20px;
	border-bottom:5px solid rgba(236,108,0,1);
	padding:10px 0 0 35px;
	background:rgba(194,193,193,.2);
	transition:all ease-out .2s;
	}
.tabA div{
	width:551px;
	height:295px;
	position:absolute;
	border:5px solid rgba(236,108,0,1);
	border-top:none;
	padding:30px;
	display:none;
	}
.onTab{
	color:#fff;
	background:rgba(236,108,0,1) !important;
	}
.tabA div img{
	float:left;
	padding:5px;
	border:#CCC 2px solid;
	margin:0 10px 0 0;
	transition:all .4s ease-out;
	}
.onDiv{
	display:block !important;
	}
.onDiv img:hover,.teacher img:hover,.payment article img:hover{
	border:2px solid #679B15;
	}
.tabA div article{
	height:164px;
	margin:0 0 20px 0;
	}
.tabA div article h3{
	padding:15px 0;
	font-family:fBack;
	}
.tabA div article p{
	text-indent:32px;
	}
.essay p{
	padding:5px 0;
	border-bottom:1px #E4E4E4 dashed;
	}
.essay p:hover{
	color:#679B15;
	}
.essay p span{
	float:right;
	}
/*teacher*/
.teacher{
	height:400px;
	width:320px;
	float:right;
	margin-top:8px;
	}
.teacher h3{
	background:#EC6C00;
	color:#fff;
	padding:3px 20px;
	font-size:16px;
	box-shadow:rgba(51,51,51,.5) -1px 3px 3px;
	}
.teacher h4{
	font-family:fBack;
	padding:6px 40px;
	border-bottom:#EC6C00 2px solid;
	font-size:16px;
	}
.teacher li{
	width:310px;
	margin:0 0 2px 0;
	height:30px;
	overflow:hidden;
	transition: height .3s ease-in;
	background:rgba(194,193,193,.1);
	}
.teacher ul{
	float:right;
	}
.teacher li p{
	padding:10px;
	text-indent:32px;
	font-size:12px;
	}
.teacher li p img,.payment article img{
	float:left;
	padding:2px;
	border:2px solid rgba(194,193,193,.2);
	transition:all 0.4s ease-out;
	margin-right:10px;
	}
.onT{
	height:200px !important;
	}
/*graduate*/
.graduate{
	width:620px;
	height:300px;
	float:left;
	}
.graduate article{
	width:310px;
	height:230px;
	padding:10px 15px;
	float:right;
	border-left:1px rgba(194,193,193,.7) solid;
	position:relative;
	}
.graduate ul{
	float:left;
	font-size:12px;
	padding:10px 10px 0 25px;
	}
.graduate h4{
	font-size:16px;
	border-bottom:2px #EC6C00 solid;
	padding-bottom:10px;
	}
.graduate ul li{
	padding:5px 10px;
	border-bottom:1px #ccc dashed;
	transition:all .3s ease-out;
	}
.graduate article img{
	position:absolute;
	transition:all .2s linear;
	padding:2px;
	border:1px rgba(194,193,193,.7) solid;
	}
.graduate article img:hover{
	transform:scale(1.2);
	-webkit-transform:scale(1.2);
	border:#679B15 1px solid;
	}
.graduate article img:nth-of-type(1){
	top:55px;
	left:30px;
	}
.graduate article img:nth-of-type(2){
	top:55px;
	left:180px;
	}
.graduate article img:nth-of-type(3){
	top:160px;
	left:30px;
	}
.graduate article img:nth-of-type(4){
	top:160px;left:180px;
	}
.graduate h4 span{
	cursor:pointer;
	font-weight:normal;
	color:#333;
	margin:5px 14px 0 0;
	float:right;
	font-size:12px
	}
/*money*/
.money{
	clear:both;
	width:960px;
	margin:0 auto;
	background:#fff;
	padding:10px 0 100px 0;
	height:600px;
	}
.payment{
	width:960px;
	height:300px;
	border-bottom:2px #EC6C00 solid;
	clear:both;
	}
.payment h3,.free h3,.graduate h3{
	padding:6px 20px;
	background:#EC6C00;
	margin-bottom:10px;
	color:#FFF;
	}
.payment article h4{
	font-family:fBack;
	font-size:16px;
	padding:10px 0;
	}
.payment article{
	float:left;
	width:458px;
	margin:0 0 35px 2px;
	font-size:12px;
	padding:0 10px;
	}
.payment article span{
	float:right;
	margin:10px 30px 0 0;
	}
.payment article p{
	text-indent:24px;
	}
/*free*/
.free{
	height:300px;
	float:right;
	margin-bottom:25px;
	width:320px;
	}
.free h3 span{
	float:right;
	font-size:12px;
	padding-top:5px;
	}
.free ul li{
	padding:5px 0 5px 20px;
	border-bottom:1px #999 dashed;
	transition:all .3s ease-out;
	}
.free ul li:hover,.graduate ul li:hover{
	background:#679B15;
	color:#fff;
	}
/**/
.drang{
	width:960px;
	margin:0 auto;
	position:relative;
	}
.problem{
	width:960px;
	color:#FFF;
	position:absolute;
	top:-50px;
	transition:all .4s ease-in;
	}
.problem:hover{
	top:-207px;
	}
.problem h3{
	background:#222;
	padding:10px 10px 10px 30px;
	width:100px;
	margin:0 auto;
	color:#fff;
	border-bottom:#FFF 1px solid;
	}
.problem article{
	float:left;
	width:219px;
	padding:10px 0 5px 20px;
	color:#fff;
	height:150px;
	background:#333;
	border-right:#FFF 1px solid;
	}
.problem article:last-of-type{
	border-right:#222 1px solid;
	}
.problem article:nth-of-type(2n){
	background:#222;
	}
.problem article h4{
	padding:5px 0;
	}
.problem article p{
	padding:5px 0 5px 15px;
	font-size:12px;
	}
.problem article p:hover{
	color:#679B15;
	}
	
/**/
#footer{
	clear:both;
	margin:0 auto;
	padding:30px 0;
	background:#222;
	position:relative;
	height:170px;
	background:-moz-linear-gradient(90deg,#222,#555);
	border-top:1px #fff solid;
	}
.info{
	width:960px;
	margin:0 auto;
	font-size:12px;
	color:#fff;
	}
.info ul{
	float:left;
	margin:0 50px 0 30px;
	border-right:1px #666 solid;
	padding-right:20px;
	}
.info ul li{
	margin:5px 0;
	}
.info article{
	float:left
	}
.info article p{
	border-right:1px #666 solid;
	padding:10px 5px;
	width:420px;
	}
